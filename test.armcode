# Engage the motors
ENGAGE

# Calibrate the controller
MIN_ANGLE SHOULDER 100
MAX_ANGLE SHOULDER 155
MIN_ANGLE ELBOW 90
MAX_ANGLE ELBOW 150

# Set the default position of the arm
SET_ANGLE ROTATION 90
SET_ANGLE SHOULDER 100
SET_ANGLE ELBOW 90
SET_ANGLE HAND 0

# Raise arm
SET_SPEED 3
SET_ANGLE SHOULDER 0
SET_ANGLE ELBOW 180
SET_ANGLE ROTATION 180
SET_ANGLE ELBOW 0
SET_ANGLE HAND 180
SET_SPEED 20
SET_ANGLE SHOULDER 180
SET_SPEED 3
SET_ANGLE HAND 0
SET_ANGLE SHOULDER 0
SET_ANGLE ELBOW 180
SET_ANGLE ROTATION 0
SET_SPEED 10
SET_ANGLE ELBOW 0
SET_ANGLE SHOULDER 180
SET_SPEED 3
SET_ANGLE HAND 180
SET_ANGLE ELBOW 0
SET_ANGLE SHOULDER 0
SET_ANGLE HAND 0
SET_ANGLE ROTATION 90
DISENGAGE
